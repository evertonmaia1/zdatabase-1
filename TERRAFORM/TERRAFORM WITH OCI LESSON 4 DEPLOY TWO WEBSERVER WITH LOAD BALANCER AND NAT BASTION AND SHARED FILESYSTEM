#CHECK THE DOCUMENT https://github.com/danilo19ee/zdatabase/blob/master/TERRAFORM/TERRAFORM%20SETUP%20ORACLE%20CLOUD%20OCI%20ENVIRONMENT
root@raspberrypi:~/foggykitchen_tf_oci_course/LESSON5_shared_filesystem-rita# cp /root/setup_oci_tf_var.sh .
root@raspberrypi:~/foggykitchen_tf_oci_course/LESSON5_shared_filesystem-rita# source setup_oci_tf_var.sh

#RUN TERRAFORM

